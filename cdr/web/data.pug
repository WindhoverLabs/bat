.row.os-theme-dark
  article.col-xs-12.col-sm-12
    header
      span.widget-icon
        i.fa.fa-table
      h2 BAT - Application Data
    div.widget-body.table-responsive
      table.table.table-condensed
            tbody
              tr
                th Timestamp
                td
                  div(data-cdr={tlm:[{name:'/BAT/HK/Timestamp'}],indicator:'text'}) ---
              tr
                th Voltage Filtered
                td
                  div(data-cdr={tlm:[{name:'/BAT/HK/VoltageFiltered'}],indicator:'text'}) ---
              tr
                th Current Filtered
                td
                  div(data-cdr={tlm:[{name:'/BAT/HK/CurrentFiltered'}],indicator:'text'}) ---
              tr
                th CellCount
                td
                  div(data-cdr={tlm:[{name:'/BAT/HK/CellCount'}],indicator:'text'}) ---
              tr
                th Scale
                td
                  div(data-cdr={tlm:[{name:'/BAT/HK/Scale'}],indicator:'text'}) ---
              tr
                th Voltage
                td
                  div(data-cdr={tlm:[{name:'/BAT/HK/Voltage'}],indicator:'text'}) ---
              tr
                th Current
                td
                  div(data-cdr={tlm:[{name:'/BAT/HK/Current'}],indicator:'text'}) ---   
              tr
                th Connected
                td
                  div(data-cdr={tlm:[{name:'/BAT/HK/Connected'}],indicator:'text'}) ---
              tr
                th Warning
                td
                  div(data-cdr={tlm:[{name:'/BAT/HK/Warning'}],indicator:'text'}) ---            
              tr
                th Discharged
                td
                  div(data-cdr={tlm:[{name:'/BAT/HK/Discharged'}],indicator:'text'}) ---            
              tr
